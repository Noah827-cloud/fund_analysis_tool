{
  "name": "fund-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "css": "tailwindcss -c tailwind.config.js -i tailwind.input.css -o resources/tailwind.min.css --minify",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,vue}\" \"*.{js,html,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue}\" \"*.{js,html,css,md,json}\"",
    "smoke": "npm run build && node scripts/smoke.mjs",
    "test": "npm run smoke",
    "check": "npm run format:check && npm run lint && npm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "animejs": "^3.2.2",
    "autoprefixer": "^10.4.20",
    "echarts": "^5.5.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "postcss": "^8.4.47",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.25"
  }
}
